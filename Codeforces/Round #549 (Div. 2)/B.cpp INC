#include<bits/stdc++.h>
#define N 200100
#define ll long long int

using namespace std;

int main(){
	string x;
	ll stack[N];
	ll top=0;
	ll temp;
	ll acm=1;
	ll acmTempA;
	ll acmTempB;
	ll j;
	ll aux;
	cin >> x;		

	stack[top++] = x[0]-'0';
	for(ll i=1;i<x.size();i++){
		temp = x[i]-'0';
		if(stack[top-1]==1){
			for(j=top-1;j>=0&&stack[top-1]==0;j--);
			acmTempA = stack[aux];
			acmTempB = stack[aux]-1;
			aux = j;
			for(j=aux+1;j<top;j++){
				acmTempA *= stack[j];
				acmTempB *= stack[j];
			}
			if(acmTempA>acmTempB){
				stack[top++] = temp;
			}
			else{
				stack[aux]--;
				for(j=aux+1;j<top;j++){
					stack[j] = 9;
				}
				stack[top++] = 9;	 
			}
		}
		if((stack[top-1]*temp>((stack[top-1]-1))*9)){
			stack[top++] = temp;
		}
		else{
			stack[top-1]--;
			stack[top++] = 9;	 
		}
	}

	for(ll i=0;i<top;i++){
		if(stack[i]==0){
			continue;
		}
		cout << stack[i] << ' ';
		acm*=stack[i];
	}
	cout << endl;
	cout << acm << endl;

	return 0;
}
